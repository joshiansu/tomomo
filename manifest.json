{
    "name": "TOMOMO GPT",
    "description": "Effortlessly Summarize, Simplify and Stay Informed with TOMOMO.",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "chat.html",
      "default_icon": "img/LOGO.png"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }],
    "background" : {
      "service_worker": "background.js"
    }
  }
  